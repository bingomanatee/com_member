{
    "route_prefix": "/admin/member",

    "options": [
        {
            "name":"twit_app_id",
            "data_type":"string",
            "default":"twit_app_id",
            "notes":"Your Twitter App ID"
        },

        {
            "name":"twit_api_key",
            "data_type":"string",
            "default":"twit_api_key",
            "notes":"Your Twitter API key"
        },

        {
            "name":"twit_app_secret",
            "data_type":"string",
            "default":"app_secret",
            "notes":"Your Twitter App Secret"
        },

        {
            "name":"twit_domain",
            "data_type":"string",
            "default":"www.domain.com",
            "notes":"the domain of your Twitter website - include prefix (www) but exclude protocol( hhtp/s://)"
        },


        {
            "name":"fb_app_id",
            "data_type":"string",
            "default":"app_id",
            "notes":"Your Facebook App ID"
        },

        {
            "name":"fb_app_secret",
            "data_type":"string",
            "default":"app_secret",
            "notes":"Your Facebook App Secret"
        },

        {
            "name":"fb_domain",
            "data_type":"string",
            "default":"www.domain.com",
            "notes":"the domain of your website - include prefix (www) but exclude protocol( hhtp/s://)"
        }

    ]
}